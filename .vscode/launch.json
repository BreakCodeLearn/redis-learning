    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CodeLens (Launch) - RedisChallenge",
            "request": "launch",
            "mainClass": "RedisChallenge",
            "projectName": "redis.connect"
        },
        {
            "type": "java",
            "name": "CodeLens (Launch) - RedisChallenge",
            "request": "launch",
            "mainClass": "redis.connection.RedisChallenge",
            "projectName": "redis.connect"
        },
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-App<redis.connect>",
            "request": "launch",
            "mainClass": "redis.connection.App",
            "projectName": "redis.connect"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-App<redisTesting>",
            "request": "launch",
            "mainClass": "redisTest.App",
            "projectName": "redisTesting"
        }
    ]
}